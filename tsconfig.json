{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "~/*": ["src/*"],
      "~~/*": ["*"]
    },
    "jsxImportSource": "preact",
    "strict": false,
    "allowJs": true,
    "include": ["components.d.ts", "auto-imports.d.ts"]
  }
}
